<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<include file="struts-default.xml"></include>
	<package name="member" namespace="/member" extends="struts-default">
		<action name="login">
			<result>/login.jsp</result>
		</action>
		<action name="home" method="login" class="br.com.yaw.struts2.action.LoginAction">
			<result>/home.jsp</result>
			<result name="input">/login.jsp</result>
		</action>
		<action name="list" method="listAll" class="br.com.yaw.struts2.action.ListAction">
			<result>/list.jsp</result>
			<result name="error">/list.jsp</result>
		</action>
		<action name="addUser">
			<result>/addUser.jsp</result>
		</action>
		<action name="add" method="addUser" class="br.com.yaw.struts2.action.UserAction">
			<result>/addUser.jsp</result>
			<result name="error">/addUser.jsp</result>
		</action>
	</package>
</struts>